{
  "name": "testing-react-apps",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test '^((?!visual).)*$'",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 7000",
    "storybook:visualTest": "start-storybook -p 7010 --ci",
    "waitStorybookAndStartVisualTest": "wait-port http://localhost:7010 && react-scripts test .visual.",
    "test:visual": "npm-run-all --parallel storybook:visualTest waitStorybookAndStartVisualTest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest-image-snapshot": "^2.11.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^2.0.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3",
    "wait-port": "^0.2.6"
  }
}
